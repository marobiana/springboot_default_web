<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.marondal.dao.TestDAO">
 	<select id="selectTests" parameterType="com.marondal.dto.TestDTO" resultType="com.marondal.dto.TestDTO">
 		SELECT 
 			`id`
 			, `name`
 		FROM
 			test
 		WHERE
 			1 = 1
 	</select>
 </mapper>